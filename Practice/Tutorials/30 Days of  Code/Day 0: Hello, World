// Test case 1 //

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch[50] = "Hello, World.";
    char he[50] = "Welcome to 30 Days of Code!";
    printf("%s\n%s", ch,he);
    return 0;
}

